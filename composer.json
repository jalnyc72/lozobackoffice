{
    "name": "digbang/backoffice",
    "description": "A set of libraries, helpers and templates for a backoffice (admin / panel) project.",
    "license": "proprietary",
    "authors": [
        {
            "name": "Digbang",
            "email": "dgovergun@digbang.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "illuminate/support": "5.4.*|5.5.*|5.6.*|5.7.*|5.8.*",
        "illuminate/view": "5.4.*|5.5.*|5.6.*|5.7.*|5.8.*",
        "illuminate/pagination": "5.4.*|5.5.*|5.6.*|5.7.*|5.8.*",
	    "illuminate/database": "5.4.*|5.5.*|5.6.*|5.7.*|5.8.*",
	    "illuminate/config": "5.4.*|5.5.*|5.6.*|5.7.*|5.8.*",
	    "illuminate/translation": "5.4.*|5.5.*|5.6.*|5.7.*|5.8.*",
	    "illuminate/validation": "5.4.*|5.5.*|5.6.*|5.7.*|5.8.*",
        "laravelcollective/html": "5.4.*|5.5.*|5.6.*|5.7.*|5.8.*",
        "digbang/fonts": "^0.1",
        "digbang/security": "^5.1",
        "digbang/utils": "^1.3",
        "maatwebsite/excel": "^3.1",
        "nette/reflection": "~2.3"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "laravel/framework": "5.8.*",
        "laravel-doctrine/fluent": "^1.1",
        "doctrine/common": "~2.5",
        "digbang/backoffice-template": "1.0.0",
        "phpspec/phpspec": "^5.0",
        "friendsofphp/php-cs-fixer": "^2.7"
    },
	"autoload": {
		"psr-4": {
			"Digbang\\Backoffice\\": "src/"
		},
        "files": [
          "src/helpers.php"
        ]
	},
	"repositories": [
		{ "url": "git@github.com:digbang/backoffice-template.git", "type": "vcs" },
        { "url": "git@github.com:digbang/SecurityAdvisories.git", "type": "vcs" }
	]
}
